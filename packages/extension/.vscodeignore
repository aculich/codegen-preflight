# Exclude source files
src/
tsconfig.json
*.ts
!*.d.ts

# Exclude development files
.vscode/
.git/
*.log
.env
.env.local

# Exclude test files
**/*.test.ts
**/*.spec.ts
**/test/
**/tests/
**/__tests__/

# Exclude source maps
*.map

# Exclude documentation that's not needed in extension
docs/
design/
*.md
!README.md

# Exclude other development files
.eslintrc*
.prettierrc*
.editorconfig
*.vsix

# CRITICAL: Exclude ALL parent directory files
# vsce scans parent directories, so we must exclude everything from ../ and ../../
../.git/
../.github/
../../.git/
../../.github/
../../.cursor/
../../.gitignore
../../.cursorignore
../../.cursorindexingignore
../../CHANGELOG.md
../../README.md
../../FEATURES.md
../../QUICKSTART.md
../../CONTRIBUTING.md
../../design/
../../docs/
../../scripts/
../../upstream/
../../node_modules/
../../package.json
../../package-lock.json
../../.vscode/
